{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "outDir": "./docs/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "target": "es2019",
    "skipLibCheck": true,
    "paths": {
      "@assets/*" : ["src/assets/*"],
      "@env/*" : ["src/environments/*"],
      "@libs/*" : ["src/assets/libs/*"],
      "@store" : ["src/app/core/store"],
      "@models/*" : ["src/app/shared/models/*"],
      "@utils" : ["src/app/shared/utils"],
      "@shared/*" : ["src/app/shared/*"],
      "@views" : ["src/app/views"],
      "@model-viewers": ["src/app/mobius-viewer/viewers"],
      "@services" : ["src/app/shared/services"],
      "@modules" : ["src/assets/core/modules"],
      "@modules/*" : ["src/assets/core/modules/*"]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2019",
      "dom"
    ],
    "module": "esnext",
    "baseUrl": "./",
    "allowSyntheticDefaultImports":true
  },
  "exclude": [
    "node_modules"
  ]
}
