{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "outDir": "./docs/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "target": "es2019",
    "skipLibCheck": true,
    "paths": {
      "@assets/*" : ["src/assets/*"],
      "@env/*" : ["src/environments/*"],
      "@store" : ["src/app/core/store"],
      "@models/*" : ["src/app/shared/models/*"],
      "@utils" : ["src/app/shared/utils"],
      "@shared/*" : ["src/app/shared/*"],
      "@views" : ["src/app/views"],
      "@model-viewers": ["src/app/mobius-viewer/viewers"],
      "@services" : ["src/app/shared/services"],
      "@design-automation/mobius-sim" : [
        "src/mobius-sim/libs"
      ],
      "@design-automation/mobius-sim-funcs" : [
        "src/mobius-sim-funcs/src"
      ],
      "@design-automation/mobius-inline-funcs" : [
        "src/mobius-inline-funcs/src"
      ],
      "@design-automation/mobius-sim/libs/verb/verb" : [
        "src/mobius-sim/libs/verb/verb"
      ],
      "@libs/*" : ["src/mobius-sim/libs/*"],

    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2019",
      "dom"
    ],
    "module": "esnext",
    "baseUrl": "./",
    "allowSyntheticDefaultImports":true
  },
  "exclude": [
    "node_modules"
  ]
}
