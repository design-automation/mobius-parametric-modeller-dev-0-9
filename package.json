{
  "name": "mobius-parametric-modeller",
  "version": "0.9.0",
  "license": "MIT",
  "config": {
    "dev_url": "https://mobius-08.design-automation.net/",
    "prod_url": "https://mobius-08.design-automation.net/"
  },
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=16384 node_modules/@angular/cli/bin/ng serve",
    "start0": "npm run moduleDocs && node --max_old_space_size=16384 node_modules/@angular/cli/bin/ng serve",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "deploy": "npm run moduleDocs && npm run build-prod && npx ngh",
    "deploy0": "npm run moduleDocs && npm run build-prod",
    "build": "ng build",
    "build-test": "ng build --configuration production && http-server ./dist",
    "build-dev": "ng build --base-href=%npm_package_config_dev_url% && rm -r ./docs && cp -r ./dist ./docs && rm -r ./dist",
    "build-prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --base-href=%npm_package_config_prod_url% && copy dist\\index.html dist\\404.html",
    "build-server": "ng build --base-href='./'",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "postinstall": "npm run moduleDocs",
    "moduleDocs": "npm run typedoc && node version.js && npm run resizeImg",
    "typedoc": "copy src\\assets\\typedoc-json\\__doc__.json src\\assets\\typedoc-json\\doc.json && typedoc --exclude **/*.spec.ts --json src/assets/typedoc-json/doc.json src/assets/core/modules",
    "resizeImg": "node ./src/assets/gallery/img_resize.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "^12.2.8",
